<h1 class="govuk-heading-xl">Stats</h1>

<table class="govuk-table">
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th class="govuk-table__header">Time period</th>
      <th class="govuk-table__header">Number of registrations</th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <% registrations.each do |reg| %>
      <tr class="govuk-table__row">
        <td class="govuk-table__header"><%= reg.fetch(:name) %></td>
        <td class="govuk-table__cell"><%= reg.fetch(:value) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
